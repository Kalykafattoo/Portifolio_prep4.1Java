import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        System.out.println("Pessoa 1 digite sua idade");
        // Cria o leiotr
        Scanner scanner = new Scanner(System.in);
        // Le a idade de cada pessoa
        int pessoa1 = scanner.nextInt();
        scanner.nextLine();
        System.out.println(" Pessoa 2 digite a sua idade");
        int pessoa2 = scanner.nextInt();
        scanner.nextLine();

        // Comparacao em que as duas pessoas precisam
        // Ser maiores de idade
        if (pessoa1 > 17 && pessoa2 > 17) {
            System.out.println("Ok. Sem Problemas! Pode seguir adiante e bem vindo ao nosso Show Bar !!");
        } else {
            System.out.println("Me desculpa, mas nao poderemos permitir a sua entrada.");
        }



    }
}
